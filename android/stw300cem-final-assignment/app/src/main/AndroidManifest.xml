<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bookcarservicing">

    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.hardware.sensor.proximity" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".Controllers.BookServicing"></activity>
        <activity android:name=".Logout" />
        <activity android:name=".Controllers.Show_Cars" />
        <activity android:name=".Controllers.UpdateUsers" />
        <activity android:name=".Controllers.BookParts" />
        <activity android:name=".Controllers.ShowClickedParts" />
        <activity android:name=".Controllers.Show_Parts" />
        <activity android:name=".Controllers.Dashboard" />
        <activity android:name=".Controllers.Registration" />
        <activity android:name=".Controllers.Login" />
        <activity
            android:name=".Controllers.SplashScreen"
            android:noHistory="true"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>